{
  "name": "lehrstellen-app",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "dev:api": "turbo run dev --filter=@lehrstellen/api",
    "dev:mobile": "turbo run dev --filter=@lehrstellen/mobile",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "db:generate": "pnpm --filter @lehrstellen/database db:generate",
    "db:migrate": "pnpm --filter @lehrstellen/database db:migrate",
    "db:seed": "pnpm --filter @lehrstellen/database db:seed",
    "db:studio": "pnpm --filter @lehrstellen/database db:studio"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.6.0",
    "prettier": "^3.4.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
